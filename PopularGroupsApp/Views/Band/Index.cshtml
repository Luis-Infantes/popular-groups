
@using PopularGroupsApp.Models
@model List<Band>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.GlobalText = "Agenda anual";
    ViewBag.Footer = "Todos los derechos reservados";
}




<div class="d-flex justify-content-center">
    
    <div class="table-responsive" style="width: 50%;">
        <table class="table table-dark table-striped table-bordered w-100">
            <thead>
                <tr>
                    <th class="justify-content-center">
                        @TempData["Tittle"]
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var band in Model)
                {
                    <tr>
                       

                        <td>
                            @Html.Partial("ConcertBands", band)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

<div class="d-flex justify-content-center my-4">
    <a href="@Url.Action("Index", "Home")" class="btn btn-outline-warning">
        Volver
    </a>
</div>









